{
  "name": "fierve.com",
  "version": "1.0.0",
  "repository": "github:fierve/fierve.com",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o dist",
    "validate": "npm run validate:tsc && npm run validate:lint && npm run validate:prettier",
    "validate:tsc": "tsc --noEmit",
    "validate:lint": "next lint",
    "validate:prettier": "prettier --ignore-path .gitignore --check ."
  },
  "dependencies": {
    "clsx": "1.1.1",
    "next": "12.0.4",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.37",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "prettier": "2.5.0",
    "typescript": "4.5.2"
  }
}
